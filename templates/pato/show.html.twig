{% extends 'base.html.twig' %}

{% block title %}Pato{% endblock %}

{% block body %}
    <h1>Pato</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Titulo</th>
                <td>{{ pato.titulo }}</td>
            </tr>
            <tr>
                <th>Descripcion</th>
                <td>{{ pato.descripcion }}</td>
            </tr>
            <tr>
                <th>Codcie</th>
                <td>{{ pato.codcie }}</td>
            </tr>
        </tbody>
    </table>

    <a  class="btn btn-info" href="{{ path('app_pato_index') }}">back to list</a>

    <a  class="btn btn-warning" href="{{ path('app_pato_edit', {'id': pato.id}) }}">edit</a>

    {{ include('pato/_delete_form.html.twig') }}
{% endblock %}
